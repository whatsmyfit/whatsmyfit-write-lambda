{
    "name": "whatsmyfit-write-lambda",
    "version": "1.0.0",
    "description": "WhatsMyFit Write Lambda functions",
    "keywords": [
        "whatsmyfit"
    ],
    "author": "Ismar Slomic",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/whatsmyfit/whatsmyfit-write-lambda.git"
    },
    "scripts": {
        "build": "yarn run lint && yarn run clean && tsc",
        "clean": "del-cli '.build/**/*'",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "tsc": "tsc",
        "decrypt:test": "serverless decrypt --stage test --password 'secretpassword'",
        "decrypt:dev": "serverless decrypt --stage dev --password 'secretpassword'",
        "test:all": "jest",
        "test:unit": "jest -c jest.config.unit.js",
        "test:integration": "jest -c jest.config.integration.js",
        "build-test:all": "yarn run build && yarn run test:all",
        "build-test:unit": "yarn run build && yarn run test:unit",
        "build-test:integration": "yarn run build && yarn run decrypt:test && yarn run decrypt:dev && yarn run test:integration"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.17",
        "@types/jest": "^23.3.11",
        "@types/node": "^10.12.18",
        "@types/supertest": "^2.0.7",
        "@types/uuid": "^3.4.4",
        "aws-sdk": "^2.382.0",
        "aws-sdk-mock": "^4.3.0",
        "del-cli": "^1.1.0",
        "jest": "^23.6.0",
        "serverless-aws-documentation": "^1.1.0",
        "serverless-dynamodb-local": "0.2.35",
        "serverless-offline": "^3.31.3",
        "serverless-plugin-typescript": "^1.1.5",
        "serverless-secrets-plugin": "^0.1.0",
        "serverless-stack-output": "^0.2.3",
        "supertest": "^3.3.0",
        "ts-jest": "^23.10.5",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.0",
        "typescript": "^3.2.2",
        "typescript-eslint-parser": "^21.0.2"
    },
    "dependencies": {
        "http-status-codes": "^1.3.0",
        "uuid": "^3.3.2"
    }
}
